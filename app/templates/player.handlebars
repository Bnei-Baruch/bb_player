url: {{currentStream.url}}
  {{!view App.PlayerWrapperView}}
{{#if isIcecast}}
    i{{render 'icecast' currentStream}}i
{{/if}}
{{#if isHls}}
    i{{render 'hls' currentStream}}i
{{/if}}
{{#if isFlash}}
    i{{render 'flash' currentStream}}i
{{/if}}
Quality: {{view Ember.Select 
            contentBinding="controller.content"
            optionValuePath="content.id"
            optionLabelPath="content.quality"
            selectionBinding="controller.currentStream"
         }}
